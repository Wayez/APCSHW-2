import java.util.*;
import java.io.*;

public class Point{
    private int x, y, num;
    private Point next;
    public Point(int x, int y, int num){
	this.x=x;
	this.y=y;
	this.num=num;
    }
    public int getX(){
	return x;
    }
    public int getY(){
	return y;
    }
    public int getNum(){
	return num;
    }
}
	 
    
